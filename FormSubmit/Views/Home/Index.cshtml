@{
    ViewData["Title"] = "Index";
}
@model User
<div class="contianer">
    <h1>Enter your Information</h1>
</div>
<hr>
<div class="contianer">
    <form asp-action="Create" asp-controller="Home" method="post">
        <div class="form-group">
            <span asp-validation-for="first_name"></span>
            <label asp-for="first_name"></label>
            <input asp-for="first_name">
        </div>
        <div class="form-group">
            <span asp-validation-for="last_name"></span>
            <label asp-for="last_name"></label>
            <input asp-for="last_name">
        </div>
        <div class="form-group">
            <span asp-validation-for="age"></span>
            <label asp-for="age"></label>
            <input asp-for="age">
        </div>
        <div class="form-group">
            <span asp-validation-for="Email"></span>
            <label asp-for="Email"></label>
            <input asp-for="Email">
        </div>
        <div class="form-group">
            <span asp-validation-for="Password"></span>
            <label asp-for="Password"></label>
            <input asp-for="Password">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>