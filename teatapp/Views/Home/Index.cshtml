@{
    ViewData["Title"] = "Dojo Survey";
}
@model Survey

<h1>Dojo Survey</h1>
<hr>
<form asp-action="Create" asp-controller="Home" method="post">
    <div class="form-group">
        <span asp-validation-for="Fullname"></span>
        <label asp-for="FullName"></label>
        <input asp-for="FullName">
    </div>
    <div class="form-group">
        <span asp-validation-for="DojoLocation"></span>
        <label asp-for="DojoLocation"></label>
        <input asp-for="DojoLocation">
    </div>
    <div class="form-group">
        <span asp-validation-for="Location"></span>
        <label asp-for="Location"></label>
        <input asp-for="Location">
    </div>
    <div class="form-group">
        <span asp-validation-for="Comment"></span>
        <label asp-for="Comment"></label>
        <input asp-for="Comment">
    </div>
</form>

